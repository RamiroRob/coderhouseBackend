<div>
    <h1>Productos</h1>
    <table>
        <thead>
            <th>Title</th>
            <th>Description</th>
            <th>Price</th>
            <th>Stock</th>
        </thead>
        <tbody>
            {{#each products}}
            <tr>
                <td>{{title}}</td>
                <td>{{description}}</td>
                <td>{{price}}</td>
                <td>{{stock}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>